data Bool : Type {
  True : Bool,
  False : Bool
}

data Nat : Type {
  Z : Nat,
  S : (n :L Nat) -> Nat
}

data Vect : Nat -> Type {
  Nil : Vect Z,
  Cons : (n : Nat) -> Bool -> Vect n -> Vect (S n)
}

length : (n : Nat) -> Vect n -> Nat {
  length {Z} Nil ~> Z,

  forall (n : Nat) (x : Bool) (xs : Vect n)
  length ({S} n) (Cons [n] x xs) ~> S (length n xs)
}

main : Nat ~>
  length (S Z) (Cons Z True Nil).
